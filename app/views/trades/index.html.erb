<p id="notice"><%= notice %></p>

<h1>Trades</h1>

<table>
  <thead>
    <tr>
      <th>Symbol</th>
      <th>Long or Short</th>
      <th>Entry</th>
      <th>Stoploss</th>
      <th>Targetprofit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trades.each do |trade| %>
      <tr>
        <td><%= trade.symbol %></td>
        <td><%= trade.long_short %></td>
        <td><%= trade.entry %></td>
        <td><%= trade.stopLoss %></td>
        <td><%= trade.targetProfit %></td>
        <td><%= link_to 'Show', trade %></td>
        <td><%= link_to 'Edit', edit_trade_path(trade) %></td>
        <td><%= link_to 'Destroy', trade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trade', new_trade_path %>
